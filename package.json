{
  "name": "jsui-language-support",
  "displayName": "JSUI Language Support",
  "description": "Javascript language extension for intuitive UI development.",
  "publisher": "Aleh Belski",
  "version": "0.0.1",
  "main": "./client.js",
  "activationEvents": [
    "onLanguage:jsui"
  ],
  "engines": {
    "vscode": "^1.80.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alehoppai/jsui-vscode-syntax-support"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "jsui",
        "aliases": [
          "jsui",
          "jsui"
        ],
        "extensions": [
          ".jsui"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "jsui",
        "scopeName": "source.js.jsui",
        "path": "./syntaxes/jsui.tmLanguage.json"
      }
    ]
  },
  "icon": "media/jsui-extension-icon.png",
  "dependencies": {
    "vscode-languageserver": "^9.0.1"
  }
}
